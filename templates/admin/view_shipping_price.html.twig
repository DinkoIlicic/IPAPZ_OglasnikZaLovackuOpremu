{% extends '/admin/header.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" type="text/css" href="{{ asset('css/search.css') }}">
{% endblock %}
{% block body %}
    <div>
        {{ form_start(defaultForm) }}
            {{ form_row(defaultForm.price) }}
            <button type="submit" class="btn btn-primary" formnovalidate>Update</button>
        {{ form_end(defaultForm) }}
    </div><br /><br /><br />
    <div>
        {{ form_start(form) }}
            {{ form_row(form.query) }}
            {{ form_row(form.price) }}
            <button type="submit" class="btn btn-primary" formnovalidate>Update</button>
        {{ form_end(form) }}
    </div><br /><br /><br />
    <div>
        {{ form_start(formCsv) }}
            {{ form_row(formCsv.file) }}
            <button type="submit" class="btn btn-primary" formnovalidate>Update</button>
        {{ form_end(formCsv) }}
    </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('js/bloodhound.jquery.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/typeahead.jquery.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/shipping.js') }}"></script>
{% endblock %}